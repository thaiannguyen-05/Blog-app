  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev:db:up": "docker-compose -f docker-compose.dev.yml up -d --remove-orphans",
    "dev:db:down": "docker-compose -f docker-compose.dev.yml down",
    "dev:db:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "dev:clean": "docker-compose -f docker-compose.dev.yml down -v && docker system prune -f",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "dev": "pnpm run dev:db:up && sleep 10 && pnpm run prisma:migrate && pnpm run start:dev",
    "dev:fresh": "pnpm run dev:clean && pnpm run dev:db:up && sleep 15 && pnpm run prisma:migrate && pnpm run start:dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\""
  },
